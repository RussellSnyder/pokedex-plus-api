{
  "name": "pokedex-plus-api",
  "version": "1.0.0",
  "description": "Building Relationships Across Generations",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "load:pokemon": "npm run build && node build/scripts/loadPokemon.js",
    "load:generations": "npm run build && node build/scripts/loadGenerations.js",
    "lint": "eslint . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.14",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.flattendeep": "^4.4.6",
    "@types/node": "^12.7.2",
    "@types/stats-lite": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "nodemon": "^2.0.7",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "run-script-os": "^1.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "eslint-plugin-jest": "^24.1.0",
    "express": "^4.17.1",
    "jest": "^26.5.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.flattendeep": "^4.4.0",
    "stats-lite": "^2.2.0",
    "ts-jest": "^26.4.1"
  }
}
